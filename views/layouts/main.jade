!!!5
html(ng-app)
	head
		meta(charset="iso-8859-1")
		meta(http-equiv="X-UA-Compatible" content="IE=edge")
		meta(name="viewport" content="width=device-width, initial-scale=1.0")
		meta(name="description" content="")
		meta(name="author" content="")
		title #{title} - PolimueveT
		link(rel='stylesheet', href='/css/bootstrap.min.css')
		link(rel='stylesheet', href='/css/nav-bar-top.css')
		link(rel='stylesheet', href='/css/estilo.css')
		link(rel='stylesheet', href='/css/bootstrap-datetimepicker.min.css')
		link(rel='stylesheet', href='/css/jquery-ui-1.10.3.custom.min.css')
			
		// if lt IE 9
			script(src="https://oss.maxcdn.com/libs/html5shiv/3.7.0/html5shiv.js")
			script(src="https://oss.maxcdn.com/libs/respond.js/1.3.0/respond.min.js")
	body
		.navbar.navbar-default.navbar-fixed-top
			.container
				.navbar-header
					button(type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse")
						span.icon-bar
						span.icon-bar
						span.icon-bar
					a.navbar-brand(href="#") PolimueveT
				.navbar-collapse.collapse
					ul.nav.navbar-nav
						li
							a(href="/") Inicio
						li 
							a(href="/estado-parking") Estado Parking
						li 
							a(href="/trayectos") Trayectos
						li
							a(href="/crear-trayecto") Crear trayecto
						li
							a(href="/mis-trayectos") Mis trayectos
						li
							a(href="/registrar") Registrar
						li
							a(href="/Mi Cuenta") Mi Cuenta
		.container
				block content

		script(src="/js/jquery.js")
		script(src="/js/bootstrap.min.js")
		script(src="/js/angular.min.js")
		script(src="/js/underscore-min.js")
		script(src="/js/bootstrap-datetimepicker.min.js")
		script(src="/js/moment-with-langs.min.js")
		script(src="/js/jquery-ui-1.10.3.custom.min.js")
		script.
				moment.lang('es');
				$(function() {
						$('.timePicker').datetimepicker({
								pickDate:false,
								pickSeconds: false
						});

						$('.datePicker').datetimepicker({
								pickTime:false,
								weekStart: 1,
						});
				});
		block script